public class contatoreNani extends thread {
    public void run() {
        System.out.println(Thread.currentThread().getName());
        for(int i = 0; i < 7: i++){
            System.out.print((i + 1) + " ");
        }
    }
}


public class ProvaNani{
    public static void main (String args){
        ContaNani thr1 = new ContaNani();
        thr1.start();
        System.out.println(Thread.currentThread().getName());
    }
}




Thread2Nani

class contatoreNani2 extends Thread {
    public contatoreNani2(String nome){
        super();
        setName(nome);
    }
    public void run(){
     for(int i = 0; i < 7; i++){
     System.out.printl((i+1)+" "+getNam());    
        }  
    }
}

class ProvaNani2{
    public static void main (String args[]){
        Thread thr1 = new ContatoreNani2("tuzia");
        Thread thr2 = new ContatoreNani2("tumadre");
        thr1.start();
        thr2.start();
    }
}
